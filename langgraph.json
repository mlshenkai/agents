{
  "config_schemas": {
    "agent": {
      "type": "object",
      "properties": {
        "model_name": {
          "type": "string",
          "default": "gpt-4o-mini",
          "description": "LLM模型名称"
        },
        "temperature": {
          "type": "number",
          "default": 0.1,
          "description": "模型温度参数"
        },
        "max_tokens": {
          "type": "integer",
          "default": 1000,
          "description": "最大输出token数"
        }
      }
    }
  },
  "dependencies": [
    "./my_agent"
  ],
  "environment_variables": [
    "OPENAI_API_KEY",
    "ANTHROPIC_API_KEY"
  ]
}